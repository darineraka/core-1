  "license": "MIT",
  "homepage": "http://newup.io",
  "support": {
    "issues": "https://github.com/newup/core/issues",
    "source": "https://github.com/newup/core"
  },
  "authors": [
    {
      "name": "Johnathon Koster",
      "email": "john@stillat.com"
    }
  ],
  "require": {
    "laravel/framework": "5.*",
    "twig/twig": "~1.0",
    "symfony/yaml": "2.6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mikey179/vfsStream": "1.5.*"
  },
  "autoload": {
    "files": [
      "src/Support/helpers.php"
