{
  "name": "newup/newup",
  "description": "PHP Package Generator",
  "require": {
    "laravel/framework": "5.*",
    "twig/twig": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "files": [
      "src/Support/helpers.php"
    ],
    "psr-4": {
      "NewUp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NewUp\\Tests\\": "tests/"
    }
  }
}